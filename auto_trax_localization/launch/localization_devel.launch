<?xml version="1.0"?>
<launch>
    <node pkg="auto_trax_localization" name="localization_devel" type="localization_node" output="screen">
      <rosparam command="load" file="$(find auto_trax_localization)/config/params.yaml" />
    </node>

    <node pkg="map_server" name="cab_map" type="map_server" args="$(find auto_trax_localization)/data/map/cab.yaml" output="screen" />

    <node pkg="rviz" name="rviz" type="rviz" args="-d $(find auto_trax_localization)/data/map/cab.rviz" />

    <node pkg="rosbag" name="cab_bag" type="play" args="$(find auto_trax_localization)/data/bag/cab_full_run_bag -l -s 50" />
</launch>
