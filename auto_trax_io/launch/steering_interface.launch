<?xml version="1.0"?>

<launch>
  
  <!-- Start "auto_trax_io" to provide IO interface for auto_trax  -->
  <node name="servo_driver_interface" pkg="auto_trax_io" type="servo_driver_interface" output="screen"/>

  <!-- Start "rtimulib_ros" to provide IMU topics. >
  <node name="rtimulib_ros_node" pkg="rtimulib_ros" type="rtimulib_ros"/ -->

  <!-- Start parameters --> 
  <rosparam command="load" file="$(find auto_trax_io)/io_params.yaml"/>

</launch>
